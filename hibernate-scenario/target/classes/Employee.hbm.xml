<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>
        
   <class name="mavenintro.hibernate.Employee.Employee" table="Employee" schema="hibschema">
   <id name="empid" column="EMP_ID">
   <generator class="native"/><!-- primary key generator -->
   </id>
   <property name="empNmae" column="EMP_NAME" not-null="true"/>
   <property name="empDept"></property> 
       
   <property name="salary" not-null="true"></property>
   </class>
        
        
        
        </hibernate-mapping>