<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
        
<hibernate-mapping>
        
   <class name="mavenintro.hibernate.hibernate_scenario.Student" table="STUDENT" schema="hibschema">
   <id name="id">
   <generator class="native"/><!-- primary key generator -->
   </id>
   <property name="name"></property>
   <property name="date"></property> 
   <property name="course"></property>    
   <property name="marks"></property>
   </class>
        
        
        
        </hibernate-mapping>